{
    "eslintConfig": {
      "extends": ["react-app", "react-app/jest", "airbnb-typescript"],
      "parserOptions": {
        "project": "./tsconfig.eslint.json"
      },
      "rules": {
        "additional-rule": "warn"
      },
      "overrides": [
        {
          "files": ["**/*.ts?(x)"],
          "rules": {
            "additional-typescript-only-rule": "warn"
          }
        }
      ]
    }
  }